    "version": "0.2.0",
    "configurations": [
        


    {
        "type": "java",
        "name": "Attach",
        "request": "attach",
        "hostName": "localhost",
        "port": "<debug port of the debuggee>"
    },
        {
            "type": "java",
            "name": "TaskManagerApp",
            "request": "launch",
            "mainClass": "com.taskmanager.TaskManagerApp",
            "projectName": "desktop-assistant"
        },
        {
            "type": "java",
            "name": "MainUI",
            "request": "launch",
            "mainClass": "com.taskmanager.MainUI",
            "projectName": "desktop-assistant"
        },
        {
            "type": "java",
            "name": "MainUI",
            "request": "launch",
            "mainClass": "com.taskmanager.MainUI",
            "projectName": "task-manager"
        },
        {
            "type": "java",
            "name": "Main",
            "request": "launch",
            "mainClass": "com.taskmanager.Main",
            "projectName": "task-manager"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "TaskManagerApp",
            "request": "launch",
            "mainClass": "com.taskmanager.TaskManagerApp",
            "projectName": "task-manager",
            "vmArgs": "--module-path \"C:/Program Files/Java/javafx-sdk-24.0.1/lib\" --add-modules javafx.controls,javafx.fxml"
        }
    ]
